<p id="notice"><%= notice %></p>

<h1>Nyans</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nyans.each do |nyan| %>
      <tr>
        <td><%= link_to 'Show', nyan %></td>
        <td><%= link_to 'Edit', edit_nyan_path(nyan) %></td>
        <td><%= link_to 'Destroy', nyan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nyan', new_nyan_path %>
